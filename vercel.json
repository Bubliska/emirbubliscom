{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/contact.html",
      "dest": "/contact.html"
    },
    {
      "src": "/who-am-i.html",
      "dest": "/who-am-i.html"
    },
    {
      "src": "/works.html",
      "dest": "/works.html"
    },
    {
      "src": "/index.html",
      "dest": "/index.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    },
    {
      "src": "/who-am-i",
      "dest": "/who-am-i.html"
    },
    {
      "src": "/works",
      "dest": "/works.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

